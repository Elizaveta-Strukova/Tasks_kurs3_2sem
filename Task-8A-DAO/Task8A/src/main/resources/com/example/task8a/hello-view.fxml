<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="420.0" prefWidth="655.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.task8a.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="454.0" prefWidth="576.0">
        <children>
            <TableView fx:id="table" layoutX="9.0" layoutY="33.0" prefHeight="238.0" prefWidth="349.0">
                <columns>
                    <TableColumn prefWidth="75.0" text="C1" />
                    <TableColumn prefWidth="75.0" text="C2" />
                </columns>
            </TableView>
            <TextField fx:id="name" layoutX="436.0" layoutY="43.0" prefHeight="26.0" prefWidth="150.0" promptText="name" />
            <TextField fx:id="time" layoutX="436.0" layoutY="77.0" promptText="time" />
            <TextField fx:id="status" layoutX="435.0" layoutY="113.0" prefHeight="26.0" prefWidth="150.0" promptText="status" />
      
          <RadioButton fx:id="radioh2" layoutX="447.0" layoutY="174.0" mnemonicParsing="false" onAction="#getBD" prefHeight="26.0" prefWidth="123.0" text="H2" />
          <RadioButton fx:id="radiofile" layoutX="448.0" layoutY="149.0" mnemonicParsing="false" onAction="#getBD" prefHeight="26.0" prefWidth="73.0" text="File" />
          <Button layoutX="440.0" layoutY="232.0" onAction="#onButtonClick" prefHeight="26.0" prefWidth="107.0" text="Добавить!" />
        </children>
    </AnchorPane>
</VBox>
